      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
	Title="_0">
    <Page.Resources>
        <RectangleGeometry x:Key="0" Rect="-256,-128, 512,256" />
        <RectangleGeometry x:Key="1" Rect="-288,-160, 576,320" />
        <EllipseGeometry x:Key="UnitCircle" Center="0,32" RadiusX="32" RadiusY="32" />
        <RadialGradientBrush x:Key="c0" RadiusX="1" RadiusY="1" >
            <GradientStop Color="Transparent" Offset="1" />
            <GradientStop Color="Black" Offset="0" />
        </RadialGradientBrush>
    </Page.Resources>
    <Canvas>
        <Path Data="{StaticResource 1}" Fill="Black"/>
        <Path Data="{StaticResource 0}" Fill="{StaticResource c0}" />
